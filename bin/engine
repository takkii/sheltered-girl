#!/usr/bin/env ruby
# frozen_string_literal: true

# vim: filetype=ruby

lib = File.expand_path('lib', __dir__)
$LOAD_PATH.unshift(lib) unless $LOAD_PATH.include?(lib)

begin
require 'zinbeijett'
require 'prop/druby_reci'
rescue Exception => e
	#e.backtrace
	puts '例外を補足しました...'
end


Encoding.default_internal = 'UTF-8'

GC.stress = true

__END__
